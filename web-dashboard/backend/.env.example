# Backend Environment Variables
# Скопируйте этот файл в .env и заполните своими значениями

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# JWT Secret (измените на случайную строку)
JWT_SECRET_KEY=your_super_secret_jwt_key_change_this_in_production

# Database
DATABASE_URL=sqlite:///./wild_analytics.db

# API Keys
SERPER_API_KEY=your_serper_api_key_here
MPSTATS_API_KEY=your_mpstats_api_key_here
YOUTUBE_API_KEY=your_youtube_api_key_here
VK_SERVICE_KEY=your_vk_service_key_here

# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_here
ADMIN_ID=123456789

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=False

# CORS Origins
ALLOWED_ORIGINS=http://localhost:3000,https://crm.samp.business
